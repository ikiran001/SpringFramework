package com.in28Minutes.learnspringframework.game;

public class SuperContraGame {

	public void up(){
		System.out.println("Jump");
	}

	public void down(){
		System.out.println("sit down");
	}

	public void left(){
		System.out.println("Back");
	}

	public void right(){
		System.out.println("Shoot a bullet");
	}



}
