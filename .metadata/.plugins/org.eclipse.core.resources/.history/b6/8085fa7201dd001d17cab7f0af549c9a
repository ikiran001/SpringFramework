package com.in28Minutes.learnspringframework;

import org.springframework.context.annotation.Bean;
import org.springframework.context.annotation.Configuration;

@Configuration
public class HelloWorldConfiguration {
	
	@Bean
	public String name() {
		return "Kiran";
	}
	
	@Bean
	public int age() {
		return "Kiran";
	}
	
	

}
